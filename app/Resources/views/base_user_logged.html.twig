    {% block stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <style type="text/css">
        nav ul {list-style: none; margin:0px}
        nav ul li{display:inline-block; line-height: 40px; margin:0 10px;}
    </style>
</head>
<body>
    <nav class="navbar navbar-default">
        <div class="container-fluid">
            <div class="collapse navbar-collapse" id="admin-navbar-collapse-1">

                {{ knp_menu_render('PolcodeProductBundle:Builder:mainMenu') }}
            </div>
        </div>
    </nav>
{% block body %}{% endblock %}
{% block javascripts %}{% endblock %}
</body>
</html>
